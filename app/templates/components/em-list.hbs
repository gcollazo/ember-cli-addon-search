<thead>
  {{partial "-table-header"}}
</thead>

<tbody>
  {{#each currentPageContent as |pkg|}}
    {{em-pkg pkg=pkg}}
  {{else}}
    <td colspan="6">
      <p class="not-found">Nothing found for "{{query}}"</p>
    </td>
  {{/each}}

  {{em-pagination
    list=sortedPackages
    page=page
    limit=limit
    previous=previous
    next=next
  }}
</tbody>
